@model IEnumerable<Lesson3.Models.Product>

<table>
<tr>
    <th>
        ID
    </th>
    <th>
        Name
    </th>
    <th>
        Price
    </th>
    <th>
        Creation Date
    </th>
</tr>    
    @foreach (var product in Model)
    {
        <tr>
            <td>@product.ID</td>
            <td>@product.Name</td>
            <td>$ @product.Price</td>
            <td>@product.CreatedDate.ToLongDateString()</td>
        </tr>
    }
</table>
